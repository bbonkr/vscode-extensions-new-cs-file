{
  "name": "kr-bbon-vscode-plugins-newcsfile",
  "displayName": "NewCsFile",
  "description": "New cs file for dotnet",
  "version": "0.1.0",
  "license": "MIT",
  "publisher": "bbonkr",
  "author": {
    "name": "Pon Cheol Ku (구본철)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bbonkr/vscode-extensions-new-cs-file"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "productivity",
    "dotnet",
    "file",
    "new file",
    "new cs file",
    "new cs file with namespace"
  ],
  "activationEvents": [
    "onCommand:krbbonvscodeextension.newCsFile"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "krbbonvscodeextension.newCsFile",
        "title": "New CS file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:lib": "mocha -r ts-node/register src/**/*.spec.ts",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vsce": "^2.13.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.3",
    "xpath": "^0.0.32"
  }
}
